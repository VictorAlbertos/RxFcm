<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="victoralbertos.io.rxfcm">

    <application
        android:name="victoralbertos.io.rxfcm.presentation.RxSampleApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="victoralbertos.io.rxfcm.presentation.StartActivity"
            android:label="Start activity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="victoralbertos.io.rxfcm.presentation.HostActivityIssues"
            android:label="Issues"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"/>
        <activity
            android:name="victoralbertos.io.rxfcm.presentation.HostActivitySupplies"
            android:label="Supplies"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="victoralbertos.io.rxfcm.presentation.nested_fragment.HostActivityNestedFragment"
            android:label="Nested supplies"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"/>
    </application>

</manifest>
